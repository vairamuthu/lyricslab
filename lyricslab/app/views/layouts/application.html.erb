    					<div class="alert  alert-<%= flash_class(level) %>">
      					<a href="#" class="close" data-dismiss="alert">&times;</a>
      						<%= flash[level] %>
							    </div>
							  <% end %>
							<% end %>
                        	</div>
                        	
                    </div>
                </div>



